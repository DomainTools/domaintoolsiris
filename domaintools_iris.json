     {
        "action":"pivot",
        "description":"Find domains connected by any supported Iris search parameter",
        "verbose":"",
        "type":"investigate",
        "identifier":"pivot_action",
        "read_only":true,
        "parameters":{
           "query_value":{
              "description":"value to query",
              "data_type":"string",
              "contains":[
                "url",
                "domain",
                "ip",
                "email"
              ],
              "required":true,
              "primary":true,
              "order":1
           },
           "pivot_type":{
              "description":"field to pivot on",
              "data_type":"string",
              "value_list":[
                 "domain",
                 "ip",
                 "email_domain",
                 "email",
                 "nameserver_host",
                 "nameserver_domain",
                 "nameserver_ip",
                 "registrar",
                 "registrant",
                 "registrant_org",
                 "mailserver_host",
                 "mailserver_domain",
                 "mailserver_ip",
                 "redirect_domain",
                 "ssl_hash",
                 "ssl_subject",
                 "ssl_email",
                 "ssl_org",
                 "google_analytics",
                 "adsense",
                 "asn",
                 "isp_name"
              ],
              "required":true,
              "order": 0
           },
           "status":{
            "description":"return domains of this registration type",
            "data_type":"string",
            "required":false,
            "primary":true,
            "value_list": ["Any", "Active", "Inactive"],
            "default": "Any",
            "order": 2
          },
          "data_updated_after":{
           "description":"Iris records that were updated on or after midnight on this date, in YYYY-MM-DD format or relative options ( 'today', 'yesterday' )",
           "data_type":"string",
           "required":false,
           "primary":true,
           "order": 3
         },
          "tld":{
            "description":"Limit results to only include domains in a specific top-level domain (i.e. “tld=com” or “tld=ru”)",
            "data_type":"string",
            "required":false,
            "primary":true,
            "order": 4
          },
          "create_date":{
          "description":"Only include domains created on a specific date, in YYYY-MM-DD format or relative options ( 'today', 'yesterday' )",
          "data_type":"string",
          "required":false,
          "primary":true,
          "order": 5
        },
        "expiration_date":{
          "description":"Only include domains expiring on a specific date, in YYYY-MM-DD format or relative options ( 'today', 'yesterday' )",
          "data_type":"string",
          "required":false,
          "primary":true,
          "order": 6
        }
        },
        "render":{
           "width":12,
           "title":"Connected Domains",
           "type":"table",
           "height":10
        },
        "output":[
           {
              "data_path":"action_result.data.*.results.*.domain",
              "data_type":"string",
              "contains":[
                 "domain"
              ],
              "column_order":0,
              "column_name":"Connected Domain(s)"
           },
           {
            "data_path":"action_result.data.*.results.*.domain_risk.risk_score",
            "data_type":"numeric",
            "column_order":1,
            "column_name":"Risk Score"
         }
        ],
        "versions":"EQ(*)"
     },
